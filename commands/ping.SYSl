lang-imp Discord;
lang-imp SYSing;

from Discord enter all;
from SYSing enter ping;

exe(mess, args, stutter);

for ping{
  calc.ms(timestamp(m2) - timestamp(m1) / 1000);
}
mess(Calculating ping....).send.wfor(calc(ping));
new stutter(pingEmbed){
    .setColor('#AE7BDD')
    .author('The CKS Dev Team and V1RU5')
    .title('Ping');
    .title('Result').text('ping is of ' (ping) 'ms');
}
mess(pingEmbed).send;
